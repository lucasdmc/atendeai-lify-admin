# ========================================
# CONFIGURAÇÕES DAS APIS AI - COPIAR PARA .env
# ========================================

# OpenAI API (GPT-4o, Whisper, TTS)
OPENAI_API_KEY=sua_chave_openai_aqui
OPENAI_ORG_ID=sua_org_id_opcional

# Anthropic API (Claude 3.5 Sonnet)
ANTHROPIC_API_KEY=sua_chave_anthropic_aqui

# Google AI API (Gemini Pro)
GOOGLE_API_KEY=sua_chave_google_aqui
GOOGLE_PROJECT_ID=seu_project_id_aqui

# Azure OpenAI (opcional)
AZURE_OPENAI_API_KEY=sua_chave_azure_aqui
AZURE_OPENAI_ENDPOINT=seu_endpoint_azure_aqui
AZURE_OPENAI_DEPLOYMENT_NAME=seu_deployment_name_aqui

# Configurações do servidor
NODE_ENV=development
PORT=3000 